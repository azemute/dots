DOTS=".dots"

# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/${DOTS:-".zprezto"}/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/${DOTS:-".zprezto"}/init.zsh"
fi

# Customize to your needs...
VISUAL="vim"
EDITOR="vim"
